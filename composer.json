{
    "name": "jokerov/oauth2-mailru",
    "description": "Mail.ru OAuth2 Client Provider for The PHP League OAuth2-Client",
    "license": "MIT",
    "authors": [
        {
            "name": "Eugene Jokerov",
            "email": "jokerov@gmail.com"
        }
    ],
    "keywords": [
        "oauth",
        "oauth2",
        "client",
        "authorization",
        "authentication",
        "mail.ru",
        "mailru",
        "mail ru"
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.3 || ^8.0",
        "league/oauth2-client": "^2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0"
    },
    "autoload": {
        "psr-4": {
            "Jokerov\\OAuth2\\Client\\Provider\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jokerov\\OAuth2\\Client\\Test\\Provider\\": "test/"
        }
    }
}
